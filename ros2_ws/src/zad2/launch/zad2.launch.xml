<launch>
    <!-- Spustenie uzla pre generovanie trajektórie -->
    <node
        pkg="zad2"
        exec="zad2_node"
        name="zad2_node"
        output="screen">
    </node>

    <!-- Vizualizácia modelu ABB IRB 4600 -->
    <include file="$(find-pkg-share abb_model)/launch/test_irb4600.launch.xml">
        <!-- Ďalšie voliteľné argumenty pre ABB IRB 4600 model -->
    </include>

    <!-- Vizualizácia trajektórie pomocou joint_states -->
    <include file="$(find-pkg-share trajectory_visualization)/launch/trajectory_visualization.launch.xml">
        <!-- Argumenty na prispôsobenie trajektórie -->
    </include>
</launch>
